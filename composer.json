{
    "name": "infw/pug",
    "description": "Pug template engine for InFw and Zend Expressive",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0",
        "psr/container": "^1.0.0",
        "pug-php/pug": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "InFw\\Pug\\": "src/InFw/Pug"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "InFw\\Pug\\ConfigProvider"
        }
    }
}
